{
  "name": "lindesbs/minkorrekt",
  "type": "contao-bundle",
  "description": "minkorrekt-history Tools",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Stefan Lindecke",
      "homepage": "https://github.com/lindesbs"
    }
  ],
  "require": {
    "contao/core-bundle": "^4.9"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "composer/composer": "^1.0 || ^2.0",
    "phpunit/phpunit": "^5.7.26"
  },
  "autoload": {
    "psr-4": {
      "lindesbs\\minkorrekt\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/skeleton",
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "lindesbs\\minkorrekt\\Test\\": "tests/"
    }
  },
  "extra": {
    "contao-manager-plugin": "lindesbs\\minkorrekt\\ContaoManager\\Plugin"
  }
}
